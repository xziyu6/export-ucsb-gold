{
    "manifest_version": 3,
    "name": "Export UCSB GOLD",
    "version": "0.1.0",
    "description": "Export UCSB GOLD schedule as an ICS file or import into Google Calendar",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://my.sa.ucsb.edu/gold/WeeklyStudentSchedule.aspx",
                "https://my.sa.ucsb.edu/gold/WeeklyCartSchedule.aspx"
            ],
            "js": [
                "scripts/jquery-3.7.1.min.js",
                "scripts/export.js"
            ]
        },
        {
            "matches": [
                "https://my.sa.ucsb.edu/gold/RegistrationInfo.aspx"
            ],
            "js": [
                "scripts/jquery-3.7.1.min.js",
                "scripts/save_quarter_info.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "storage"
    ]
}